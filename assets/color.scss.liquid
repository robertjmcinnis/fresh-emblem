{%- capture settings_fonts -%}{{settings.font_content_family.family}},{{settings.font_accent_family.family}},{{settings.font_quote_family.family}},{{settings.font_service_footer_family.family}}{%- endcapture -%}
{%- assign fonts = settings_fonts | split: ',' | uniq -%}

{%-for variant in settings.font_base_family.variants-%}
  {{ variant | font_face }}
{%-endfor-%}

{%-if fonts contains settings.font_content_family.family -%}
  {%-for variant in settings.font_content_family.variants-%}
    {{ variant | font_face }}
  {%-endfor-%}
  {%- assign fonts = settings_fonts | join: ',' | remove: settings.font_content_family.family | split: ',' | uniq -%}
{%-endif-%}

{%-if fonts contains  settings.font_accent_family.family-%}
  {%-for variant in settings.font_accent_family.variants-%}
    {{ variant | font_face }}
  {%-endfor-%}
  {%- assign fonts = settings_fonts | join: ',' | remove: settings.font_accent_family.family | split: ',' | uniq -%}
{%-endif-%}

{%-if fonts contains settings.font_quote_family.family-%}
  {%-for variant in settings.font_quote_family.variants-%}
    {{ variant | font_face }}
  {%-endfor-%}
  {%- assign fonts = settings_fonts | join: ',' | remove: settings.font_quote_family.family | split: ',' | uniq -%}
{%-endif-%}

{%-if fonts contains settings.font_service_footer_family.family-%}
  {%-for variant in settings.font_service_footer_family.variants-%}
    {{ variant | font_face }}
  {%-endfor-%}
  {%- assign fonts = settings_fonts | join: ',' | remove: settings.font_service_footer_family.family | split: ',' | uniq -%}
{%-endif-%}

