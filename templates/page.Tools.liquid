<div id="modal-container" class="one teaTools">
  <div class="modal-background">
    <div class="modal showModal" id="teamInfo_content">
      <h2 class="modalHead">Team Info
        <i class="fa fa-info" aria-hidden="true"></i>
        <span class="version">
          <i>Supporter: teathemes@gmail.com</i>
        </span>
      </h2>
      <div class="container-fluid">
        <img src="https://cdn.dribbble.com/users/32336/screenshots/3694947/attachments/827456/user_profile.png">
      </div>
    </div>
    <div class="modal" id="bulkCreateProductsTags_content">
      <h2 class="modalHead">Bulk Create Products Tags
        <i class="fa fa-tags" aria-hidden="true"></i>
        <span class="version">
          <i>Version: 1.0 (08/22/2017)</i>
          <!--             <i>Supporter: teathemes@gmail.com</i>
<i>Developer: teathemes@gmail.com</i> -->
        </span>
      </h2>
      <div class="container-fluid">
        <div class="tabs">
          <div class="tab">
            <label>Edit tags for all products in a collection</label>
            <div class="content row">
              <div class="half">
                <div class="row nomargin">
                  <div class="item half nopadding">
                    <div class="item-label">Choose a collection</div>
                    <div class="item-select">
                      <select data-action="collection">
                        <option value="">Select a collection</option>
                        {% for coll in collections %}
                        <option value="{{coll.id}}">{{coll.title}}</option>
                        {% endfor %}
                      </select>
                    </div>
                  </div>
                  <div class="item half nopadding">
                    <div class="item-label">Choose an action</div>
                    <div class="item-select">
                      <select data-action="action">
                        <option value="add">Add</option>
                        <option value="set">Set</option>
                        <option value="remove">Remove</option>
                        <option disabled="" value="toggle">Toggle</option>
                        <option value="purge" style="background:red;color:#fff">DELETE ALL</option>
                      </select>
                    </div>
                  </div>
                </div>
                <div class="row nomargin">
                  <div class="item">
                    <ul>
                      <li>Add: Adds tags to the existing ones.</li>
                      <li>Set: Replaces tags with new ones.</li>
                      <li>Remove: Removes matching tags.</li>
                      <li>Toggle: Future Use, disabled...</li>
                    </ul>
                  </div>
                </div>
              </div>
              <div class="half">
                <div class="item">
                  <div class="item-label">Set the tag</div>
                  <textarea id="tea-form-tags" class="tea-form-control" rows="10" placeholder="Tag1,tag2..."></textarea>
                  <button id="tea-update-tags" class="btn" data-action="update_tags">Update tags</button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
