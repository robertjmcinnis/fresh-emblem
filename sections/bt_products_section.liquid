                    <ul class="tab-list owl-carousel{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="true" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'   '|remove:'  '}}'>
