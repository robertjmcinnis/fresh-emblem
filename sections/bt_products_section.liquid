              {%- assign pr_fixed = ' desktop-columns-5 tablet-columns-3 mobile-columns-1' -%}
              {%- when '6' -%}
              {%- assign grid_item_width = ' col-lg-2 col-md-2 col-sm-4 col-xs-12' -%}
              {%- assign pr_fixed = ' desktop-columns-6 tablet-columns-3 mobile-columns-1' -%}
              {%- endcase -%}
              <div class="block-top-sell padding-0">
                <ul class="box-product-list product-list-grid{{pr_fixed}}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %} owl-carousel-mobile" data-nav="false" data-dots="false" data-margin="0" data-loop="true" data-items="1" data-responsive='{"0":{"items":1,"margin":30},"{%-if settings.use_two_col_mb-%}375{%else%}480{%endif%}":{"items":2,"margin":{%-if settings.use_two_col_mb-%}10{%else%}30{%endif%}}}' {{ block.shopify_attributes }}>
