                      </li>
                      {%-if rowCheck == block.settings.count_row or forloop.last -%}
                      {%-assign rowCheck = 1-%}
                    </ul>
                    {%-else-%}
                    {%-assign rowCheck = rowCheck | plus: 1 -%}
                    {% endif %}
                    {%-endfor %}              
                  </div>
                {%-endif-%} 
              </div>
            </div>
          {%- elsif block.type == '6' -%}
            <div class="tab-product tab-product-fade-effect" {{ block.shopify_attributes }}>
              <ul class="col-md-12 box-tabs nav-tab {{block.settings.title_align}}" style="margin-bottom:{{block.settings.margin_btitle}}px">
                {%-for i in (1..6) -%}
                {%-assign products = 'products_' | append: i -%}
                {%-assign animatein = 'animatein_' | append: i -%}
                {%-if block.settings[products] != blank-%}
                <li{% if forloop.first %} class="active"{% endif %}><a data-animated='{{block.settings[animatein]}}' data-toggle="tab" href="#tab-product-{{block.settings[products]}}{{block.id}}">{{collections[block.settings[products]].title | escape }}</a></li>
                {%-endif-%}
                {%-endfor-%}
              </ul>
