                {%-for i in (1..6) -%}
                {%-assign products = 'products_' | append: i -%}
                {%-assign animatein = 'animatein_' | append: i -%}
                {%-if block.settings[products] != blank-%}
                <li{% if forloop.first %} class="active"{% endif %}><a data-animated='{{block.settings[animatein]}}' data-toggle="tab" href="#tab-product-{{block.settings[products]}}{{block.id}}">{{collections[block.settings[products]].title | escape }}</a></li>
                {%-endif-%}
                {%-endfor-%}
              </ul>
              <div class="tab-content clearfix">
                <div class="tab-container">
                  {%- for i in (1..6)-%}
                  {%- case block.settings.limit_per_row -%}
                  {%- when 2 -%}
                  {%- assign grid_item_width = ' col-lg-6 col-md-6 col-sm-6 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-2 tablet-columns-2 mobile-columns-1' -%}
                  {%- when 3 -%}
                  {%- assign grid_item_width = ' col-lg-4 col-md-4 col-sm-6 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-3 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 4 -%}
                  {%- assign grid_item_width = ' col-lg-3 col-md-3 col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-4 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 5 -%}
                  {%- assign grid_item_width = ' col-xl-5ths col-lg-5ths col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-5 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 6 -%}
                  {%- assign grid_item_width = ' col-lg-2 col-md-2 col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-6 tablet-columns-3 mobile-columns-1' -%}
                  {%- endcase -%}                  
                  {%-assign products = 'products_' | append: i-%}
                  {%-if block.settings[products] != blank -%}
                  <div id="tab-product-{{block.settings[products]}}{{block.id}}" class="tab-panel {% if forloop.first %}active{% endif %}">
                    <ul class="product-list-grid tab-list owl-carousel-mobile{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %} {{pr_fixed}}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="false" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'    '|remove:'  '}}'>
                      {%-for product in collections[block.settings[products]].products limit: block.settings.limit-%}
                      <li class="grid-item style3 mobile-slide-item{{grid_item_width}}">
                        {% include "product-card-grid", grid: block.settings.grid, container: section.settings.width_section %}
                        {%if block.settings.range_row != 0 %}<div class="clearfix" style="margin-top: {{block.settings.range_row}}px"></div>{% endif %}
                      </li>
                      {%-endfor %}
                    </ul>
                  </div>
                  {%-endif-%}
                  {%-endfor-%}
                </div>
              </div>
            </div>
          {%- elsif block.type == '4_1' -%} 
            <div class="tab-product style3">
              <div class="col-md-12">
                <div class="tab-head">
                  <h3 class="title">{{block.settings.title}}</h3>
                  <ul class="box-tabs nav-tab tab-owl-fade-effect">
                    {%-for i in (1..6) -%}
                    {%-assign products = 'products_' | append: i -%}
                    {%-assign animatein = 'animatein_' | append: i -%}
                    {%-if block.settings[products] != blank-%}
                    <li{% if forloop.first %} class="active"{% endif %}><a data-animated='{{block.settings[animatein]}}' data-toggle="tab" href="#tab-product-{{block.settings[products]}}{{block.id}}">{{collections[block.settings[products]].title | escape }}</a></li>
                    {%-endif-%}
                    {%-endfor-%}
                  </ul>
                </div>
              </div>
              <div class="tab-product col-md-12" {{ block.shopify_attributes }}>
                <div class="tab-content clearfix">
                  <div class="tab-container">
                    {%- for i in (1..6)-%}
                    {%-assign products = 'products_' | append: i-%}
                    {%-if block.settings[products] != blank -%}
                    <div id="tab-product-{{block.settings[products]}}{{block.id}}" class="tab-panel {% if forloop.first %}active{% endif %}">
                      <ul class="owl-carousel{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %} {{pr_fixed}}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="false" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'    '|remove:'  '}}'>
                        {%-for product in collections[block.settings[products]].products limit: 8-%}
                        <li class="grid-item style7{{grid_item_width}}">
                          {% include "product-card-grid", grid: block.settings.grid, container: section.settings.width_section %}
                        </li>
                        {%-endfor %}
                      </ul>
                    </div>
                    {%-endif-%}
                    {%-endfor-%}
                  </div>
                </div>
              </div>
            </div>
          {%- elsif block.type == '5' -%}
            <div class="col-md-12">
              <div class="block-featured-product" {{ block.shopify_attributes }}>
                {% if block.settings.title !=""-%}
                <div class="section-title style3 {{block.settings.title_align}}" style="margin-bottom:{{block.settings.margin_btitle}}px">
                  <h3>{{block.settings.title}}</h3>
                </div>
                {%-endif %}

                {%-if block.settings.count_row == 1 -%}
                  <ul class="owl-carousel{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="true" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'    '|remove:'  '}}'>
                    {%-for product in collections[block.settings.products].products limit: block.settings.limit-%}
                    <li class="grid-item">
                      {% assign grid = block.settings.responsive|strip_newlines|remove:'    '|remove:'  '|split: 'items": '|last|split:','|first %}
                      {% include "product-card-grid", grid: grid, container: section.settings.width_section %}
                    </li>
                    {%-endfor %}
                  </ul>
                {%-else-%}
                  {%-assign rowCheck = 1-%}
                  <div class="owl-carousel{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="true" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'   '|remove:'  '}}'>
                    {%-for product in collections[block.settings.products].products limit: block.settings.limit-%}
                    {%-if rowCheck == 1 -%}
                    <ul class="product-list-grid">
                      {% endif %}
