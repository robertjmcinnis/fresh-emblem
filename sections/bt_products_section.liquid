                  </li>
                  {%-endfor %}
                </ul>
              </div>              
              <div class="col-lg-12">
                <a class="button-loadmore" href="{{collections[block.settings.products].url}}">{{'sections.products_section.loadmore'|t}}</a>
              </div>
            {%-endif-%}
          {%- elsif block.type == '2' -%}
            <div class="section-title style2 text-center">
              <span class="sub-title">{{block.settings.stitle}}</span>
              <h3>{{block.settings.title}}</h3>
              <span class="driver"></span>
            </div>
            {% if block.settings.products != blank -%}
              <ul class="slide-product owl-carousel{% if block.settings.nav_position != '' %} {{block.settings.nav_position}}{% endif %}{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}{% if block.settings.nav_style != '' %} nav-style{{block.settings.nav_style}}{% endif -%}" data-nav="{% if block.settings.nav_style != '' %}true{% else %}false{% endif %}" data-autoplay="{{block.settings.autoplay}}" data-dots="false" data-loop="true" data-margin="30" data-responsive='{{block.settings.responsive|strip_newlines|remove:'    '|remove:'  '}}' {{ block.shopify_attributes }}>
                {%-for product in collections[block.settings.products].products limit: block.settings.limit-%}
                <li class="grid-item">
                  {% assign grid = block.settings.responsive|strip_newlines|remove:'    '|remove:'  '|split: 'items": '|last|split:','|first %}
                  {% include "product-card-grid", grid: grid, container: section.settings.width_section %}
                </li>
                {%-endfor %}
              </ul>
            {%-endif-%}     
