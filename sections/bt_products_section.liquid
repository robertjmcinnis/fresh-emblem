              <ul class="box-tabs nav-tab tab-owl-fade-effect" style="margin: 0 auto">
                {%-for i in (1..6) -%}
                {%-assign products = 'products_' | append: i -%}
                {%-assign animatein = 'animatein_' | append: i -%}
                {%-if block.settings[products] != blank-%}
                <li{% if forloop.first %} class="active"{% endif %}><a data-animated='{{block.settings[animatein]}}' data-toggle="tab" href="#tab-product-{{block.settings[products]}}{{block.id}}">{{collections[block.settings[products]].title | escape }}</a></li>
                {%-endif-%}
                {%-endfor-%}
              </ul>
              <div class="tab-content">
                <div class="tab-container">
                  {%- for i in (1..6)-%}
                  {%-assign products = 'products_' | append: i-%}
                  {%-if block.settings[products] != blank -%}
                  <div id="tab-product-{{block.settings[products]}}{{block.id}}" class="tab-panel{% if forloop.first %} active{% endif %}">
                    <ul class="owl-carousel no-overflow product-owl-no-overflow nav-style5{%unless section.settings.product_grid_layout == ''%} KiTi_Card style{{section.settings.product_grid_layout}}{% endunless %}" data-nav="true" data-autoplay="false" data-dots="false" data-loop="true" data-margin="0" data-responsive='{"0":{"items":1},"600":{"items":2},"1000":{"items":3}}'>
                      {%-for product in collections[block.settings[products]].products limit: block.settings.limit-%}
                      <li class="grid-item style3 mobile-slide-item{{grid_item_width}}">
                        {% include "product-card-grid", grid: '3', container: false %}
                      </li>
                      {%-endfor %}
                    </ul>
                  </div>
                  {%-endif-%}
                  {%-endfor-%}
                </div>
              </div>
            </div>
          {%-endif-%}
        {%-endfor-%}
