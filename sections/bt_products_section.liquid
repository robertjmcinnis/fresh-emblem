                  {%- assign pr_fixed = 'desktop-columns-2 tablet-columns-2 mobile-columns-1' -%}
                  {%- when 3 -%}
                  {%- assign grid_item_width = ' col-lg-4 col-md-4 col-sm-6 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-3 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 4 -%}
                  {%- assign grid_item_width = ' col-lg-3 col-md-3 col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-4 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 5 -%}
                  {%- assign grid_item_width = ' col-xl-5ths col-lg-5ths col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-5 tablet-columns-3 mobile-columns-1' -%}
                  {%- when 6 -%}
                  {%- assign grid_item_width = ' col-lg-2 col-md-2 col-sm-4 col-xs-12' -%}
                  {%- assign pr_fixed = 'desktop-columns-6 tablet-columns-3 mobile-columns-1' -%}
