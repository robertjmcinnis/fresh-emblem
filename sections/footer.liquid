              </div>
              <div class="widget widget_social {{block.settings.socials_design}} text-{{block.settings.content_align}}">
                <div class="content">
                  {%- include "boutique_socials", social_grid: 'social' -%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_11' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar style_5 {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget our-service {{block.settings.newsletter_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <div class="content">
                  <div class="tagcloud">
                    {%-assign colletion = collections['all']-%}
                    {%-if block.settings.use_limit_tags != blank and block.settings.tags_items != blank -%}
                      {%-assign limit_tg = colletion.all_tags.size-%}
                      {%-assign limit_check = block.settings.tags_items|strip_newline|strip| split: ','-%}
                    {%-else-%}
                      {%-assign limit_tg = block.settings.use_limit_tags | default: collections['all'].all_tags.size -%}
                    {%-endif-%}
                    {%-for item_tags in colletion.all_tags limit: limit_tg -%}
                      {%-if item_tags != ''-%}
                      {%-if block.settings.use_limit_tags != blank and block.settings.tags_items != blank -%}
                        {%-if limit_check contains item_tags-%}
                          {%-capture linkToTag -%}{{ item_tags| highlight_active_tag | link_to_tag: item_tags }}{%-endcapture-%}
                          {{linkToTag|replace: '/?constraint=','/collections/all/'}}
                        {%-endif-%}
                      {%-else-%}
                        {%-capture linkToTag -%}{{ item_tags| highlight_active_tag | link_to_tag: item_tags }}{%-endcapture-%}
                        {{linkToTag|replace: '/?constraint=','/collections/all/'}}
                      {%-endif-%}
                      {%-endif-%}
                    {%-endfor %}
                    {%- assign tag_color_footer = settings.tag_color_footer -%}
                    {%- assign tag_hover_color_footer = settings.tag_hover_color_footer -%}
                  </div>
                </div>
              </div>
            </div>
          {%- endif -%}
          {%- endfor -%}
        </div>
      </div>
    </div>
    {%- if section.blocks.size != 0 -%}
    {%- for block in section.blocks -%}
    {%- if block.type == 'footer_bottom_1' -%}
    <div class="footer-bottom">
      <div class="payment{% if section.settings.width_section %} container{% else %} container-fluid{% endif %}" style="border-top-color: {{block.settings.border_color|default:'#e0e0e0' | color_modify: 'alpha', 0.9 }}">
        <div class="">
          <div class="head">
            <strong><span style="color: {{block.settings.title_color}}">{{block.settings.footer_small_title_payment}}</span></strong>
            <span class="PlayfairDisplay">{{block.settings.footer_widget_title_payment}}</span>
          </div>
          <div class="list">
            {%-for i in (1..6)-%}
            {%-assign payment_image = 'payment_image_'| append: i -%}
            {%-assign payment_call_to_action = 'payment_call_to_action_'| append: i -%}
            {%-if block.settings[payment_image] != blank -%}
            <a href="{{block.settings[payment_call_to_action]|default: '#'}}" class="item"><img src="{{block.settings[payment_image] | img_url: 'master'}}" alt="{{image_.alt}}"></a>
            {%-endif-%}
            {%-endfor-%}
          </div>
        </div>
      </div>
    </div>
    {%- elsif block.type == 'footer_bottom_2' -%}
    <div class="footer-bottom">
      <div class="payment" style="{%- if block.settings.section_padding != blank -%}padding: {{block.settings.section_padding | replace : '-',' '}};{%- endif -%}border-top-color: {{block.settings.border_color|default:'#e0e0e0' | color_modify: 'alpha', 0.9 }}">
        {%-if block.settings.copyright != blank-%}
        <p style="margin: 0;">{{block.settings.copyright|html}}</p>
        {%-else-%}
