{%- if section.settings.use_footer -%}
{%- if settings.use_background_footer_img and settings.background_footer_img -%}
  {%- if settings.use_background_footer_parallax -%}
  {%- assign footer_bg_style = ' bg-parallax' -%}
  {%- else -%}
  {%- assign footer_bg_style = ' bg-basic' -%}
  {%- endif -%}
{%- endif -%}
{%- assign footer_service = false -%}

<footer style="font-size: {{section.settings.font_size_footer}}px;
 {%- if section.settings.section_margin_top != blank -%}margin-top: {{section.settings.section_margin_top}};{%- endif -%}
 {%- if section.settings.section_margin_bottom != blank -%}margin-bottom: {{section.settings.section_margin_bottom}};{%- endif -%}">
  <div class="footer{{footer_bg_style}}"{%- if section.settings.section_padding != blank -%} style="padding: {{section.settings.section_padding | replace : '-',' '}};"{%- endif -%}>
    {%- if section.blocks.size != 0 -%}
    {%- for block in section.blocks -%}
    {%- if block.type == 'footer_service' -%}
    <div class="{%- if section.settings.width_section -%}container{%- else -%}container-fluid{%- endif -%}">
      <div class="footer-service">
        <div class="row">
          {%-for i in (1..3)-%}
          {%-assign service_icon = 'service_icon_'| append: i -%}
          {%-assign service_text = 'service_text_'| append: i -%}
          {%-if block.settings[service_icon] != blank or block.settings[service_text] != blank -%}
          <div class="col-sm-4">
            <div class="element-icon style5{% unless forloop.last %} plus{% endunless %}">
              <div class="icon">
                <span class="fa {{block.settings[service_icon]|default: "fa-truck"}}"></span>
              </div>
              <div class="content">
                {{block.settings[service_text]}}
              </div>
            </div>
          </div>
          {%-endif-%}
          {%-endfor-%}
        </div>
      </div>
    </div>
    {%- assign footer_service = true -%}
    {%- endif -%}
    {%- endfor -%}
    {%- endif -%}
    <div class="footer-top padding-bottom-60">
      <div class="{%- if section.settings.width_section -%}container{%- else -%}container-fluid{%- endif -%}">
        <div class="row-flex justify-content-start flex-row flex-wrap">
          {%- for block in section.blocks -%}
  {%-capture block_width -%}
          {%- if block.type == 'item_1' and block.settings.block_width == '12' -%}
          offset-sm-0 col-md-8 offset-md-2 col-lg-8 offset-lg-2
          {%- if block.settings.hide_medium %} hidden-md-down{%- endif %}
          {%- if block.settings.hide_small %} hidden-sm-down{%- endif %}
          {%- if block.settings.hide_extra_small %} hidden-xs{%- else %} col-12{%- endif %}
          {%- else -%}
          col-lg-{{block.settings.block_width}}
          {%- if block.settings.hide_medium %} hidden-md-down{%- else %} col-ms-12{%- endif %}
          {%- if block.settings.hide_small %} hidden-sm-down{%- else %} col-sm-12{%- endif %}
          {%- if block.settings.hide_extra_small %} hidden-xs{%- else %} col-12{%- endif %}
          {%- endif %}
  {%-endcapture-%}
          {%- if block.type == 'item_1' -%}
            <div class="footer-sidebar {{block_width | strip_newlines | remove: '   '}}"{%- if block.type == 'item_1' and block.settings.block_width == '12' -%} style="border:none;"{%endif%} {{ block.shopify_attributes }}>
              <div class="widget contact-info widget_text text-{{block.settings.content_align}}"{%-unless footer_service-%} style="margin-top: 10px;"{%-endunless-%}>
                {%-if block.settings.use_footer_logo-%}
                <div class="logo margin-bottom-25">
                  <a href="/">
                    {%- assign image_ = block.settings.footer_logo -%}
                    {%-if image_%}
                    <img src="{{image_|img_url:'270x'}}" alt="{{image_.alt|default: 'logo'}}">
                    {%-else-%}
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 208 63" width="208" height="63">
                      <title>{{shop.name}} placeholder</title>
                      <rect width="208" height="63" style="fill:#e5e5e5"></rect>
                      <text transform="translate(55.89 42.5)" style="font-size:32px;fill:gray;font-family:Roboto-Regular, Roboto">208x63</text>
                    </svg>
                    {%-endif-%}
                  </a>
                </div>
                {%-endif-%}
                <div class="content margin-bottom-10">
                  {%-if block.settings.content_align == 'left' -%}
                  {%- assign justify_content = 'start' -%}
                  {%-elsif block.settings.content_align == 'right' -%}
                  {%- assign justify_content = 'end' -%}
                  {%-else-%}
                  {%- assign justify_content = 'center' -%}
                  {%-endif-%}
                  {%-if block.settings.footer_description != blank-%}<p>{{block.settings.footer_description}}</p>{%-endif-%}
                  {%-if block.settings.footer_widget_address_contact != ""-%}<p class="address dp-flex flex-nowrap justify-content-{{justify_content}}"><i class="fa fa-home"></i><span>{{block.settings.footer_widget_address_contact|default: shop.address.summary}}</span></p>{% endif %}
                  {%-if block.settings.footer_widget_phone_contact != ""-%}<p class="phone dp-flex flex-nowrap justify-content-{{justify_content}}" style="font-size: {{block.settings.font_size_phone}}px"><a href="tel:{{block.settings.footer_widget_phone_contact|default: shop.address.phone}}"><i class="fa fa-phone" style="font-size: {{block.settings.font_size_phone}}px;color:{{block.settings.phone_color}}"></i> <span style="color:{{block.settings.phone_color}}">{{block.settings.footer_widget_phone_contact|default: shop.address.phone}}</span></a></p>{% endif %}
                  {%-if block.settings.footer_widget_email_contact != ""-%}<p class="mail dp-flex flex-nowrap justify-content-{{justify_content}}"><a href="mailto:{{block.settings.footer_widget_email_contact|default: shop.address.email}}"><i class="fa fa-envelope"></i><span>{{block.settings.footer_widget_email_contact|default: shop.address.email}}</span></a></p>{% endif %}
                </div>
                {%-assign footer_qlink = block.settings.footer_qlink-%}
                {%-if footer_qlink != blank-%}
                <ul class="footer-menu margin-bottom-25">
                  {%-for link in linklists[footer_qlink].links-%}
                  <li><a href="{{link.url}}">{{link.title | capitalize }}</a></li>
                  {%-endfor-%}
                </ul>
                {%-endif-%}
              </div>
            </div>
          {%- elsif block.type == 'item_2' -%}
            <div class="footer-sidebar footer_links {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget style3 text-{{block.settings.content_align}} margin-bottom-25">
                <span class="text-primary PlayfairDisplay">{{block.settings.stitle}}</span>
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <div class="content">
                  {%-assign footer_link = block.settings.footer_link-%}
                  {%-if footer_link != blank-%}
                  <ul{% if block.settings.content_align == 'right' %} dir="rtl"{% endif %}>
                    {%-for link in linklists[footer_link].links-%}
                    <li><a href="{{link.url}}">{{link.title | capitalize }}</a></li>
                    {%-endfor-%}
                  </ul>
                  {%-endif-%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_3' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget contact-info contact_info_1 text-{{block.settings.content_align}} margin-bottom-25">
                <span class="text-primary PlayfairDisplay">{{block.settings.stitle}}</span>
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <div class="content">
                  {%-if block.settings.footer_widget_address_contact != ""-%}<p class="address row-flex margin-right-0 margin-left-0 margin-bottom-25 flex-nowrap justify-content-{{justify_content}}"><span>{{block.settings.footer_widget_address_contact|default: shop.address.summary}}</span></p>{%-endif-%}
                  {%-if block.settings.footer_widget_phone_contact != ""-%}<p class="phone row-flex margin-right-0 margin-left-0 margin-bottom-25 flex-nowrap justify-content-{{justify_content}}" style="font-size: {{block.settings.font_size_phone}}px"><a href="tel:{{block.settings.footer_widget_phone_contact|default: shop.address.phone}}"><i class="fa fa-phone" style="font-size: {{block.settings.font_size_phone}}px;color:{{block.settings.phone_color}}"></i><span style="color:{{block.settings.phone_color}}"> {{block.settings.footer_widget_phone_contact|default: shop.address.phone}}</span></a></p>{%-endif-%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_4' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget contact-info text-{{block.settings.content_align}} margin-bottom-25">
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <div class="content">
                  {%-if block.settings.footer_widget_address_contact != ""-%}<p class="address dp-flex flex-nowrap justify-content-{{justify_content}}"><strong>{{'layout.footer.address_label'|t}} </strong> <span> {{block.settings.footer_widget_address_contact|default: shop.address.summary}}</span></p>{%-endif-%}
                  {%-if block.settings.footer_widget_phone_contact != ""-%}<p class="phone dp-flex flex-nowrap justify-content-{{justify_content}}"><strong>{{'layout.footer.phone_label'|t}} </strong><a href="tel:{{block.settings.footer_widget_phone_contact|default: shop.address.phone}}"> <span> {{block.settings.footer_widget_phone_contact|default: shop.address.phone}}</span></a></p>{%-endif-%}
                  {%-if block.settings.footer_widget_email_contact != ""-%}<p class="mail dp-flex flex-nowrap justify-content-{{justify_content}}"><strong>{{'layout.footer.email_label'|t}} </strong><a href="mailto:{{block.settings.footer_widget_email_contact|default: shop.address.email}}"> <span> {{block.settings.footer_widget_email_contact|default: shop.address.email}}</span></a></p>{%-endif-%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_5' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget widget_social {{block.settings.socials_design}} text-{{block.settings.content_align}}">
                <div class="content">
                  <span class="text-primary PlayfairDisplay">{{block.settings.stitle}}</span>
                  <h3 class="widget-title">{{block.settings.title}}</h3>
                  {%- assign social_grid =  'dp-flex flex-nowrap justify-content-' | append: justify_content -%}
                  {%-include 'boutique_socials', social_grid: social_grid-%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_6' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget widget_social {{block.settings.socials_design}} text-{{block.settings.content_align}}">
                <div class="content">
                  <h3 class="widget-title">{{block.settings.title}}</h3>
                  {%- assign social_grid =  'dp-flex flex-nowrap justify-content-' | append: justify_content -%}
                  {%-include 'boutique_socials', social_grid: social_grid-%}
                </div>
              </div>
              <div class="widget widget_text text-{{block.settings.content_align}} margin-bottom-25">
                <h3 class="widget-title">{{block.settings.title2}}</h3>
                <div class="content">
                  <img src="{{block.settings.footer_payment|img_url: '300x'}}" alt="">
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_7' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget fick_photo {{block.settings.block_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <div id="flickr_badge_wrapper" class="fick-photo">
                  <script type="text/javascript" src="{{settings.badge_code}}"></script>
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_8' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget fick_photo {{block.settings.block_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title}}</h3>
                <ul id="instafeed" class="fick-photo hidden"></ul>
                <script type="text/javascript">
                  {%- assign instafeed_img = '{{image}}' -%}
                  {%- assign instafeed_link = '{{link}}' -%}
                  document.addEventListener('DOMContentLoaded', function(event){
                    var feed = new Instafeed({
                      target: 'instafeed',
                      get: 'user',
                      userId: '{{settings.user_id|default: '3311466850'}}',
                      accessToken: '{{settings.accessToken_instagram|default: '3311466850.1677ed0.84b4d7bf4b6f49b4887a8fe93707d9b5'}}',
                      sortBy: 'most-recent',
                      limit: {{settings.limit_instagram|default: '8'}},
                      template: '<li><a href="{{instafeed_link}}"><img class="lazyload" data-src="{{instafeed_img}}" /></a><li>'
                    });
                    feed.run();
                  })
                </script>
              </div>
            </div>
          {%- elsif block.type == 'item_9' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar {{block.settings.block_style}}{%if block.settings.use_dark %} style7{% endif %} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget widget_newletter {{block.settings.newsletter_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title}}</h3>
                {%- if settings.newslatter_mailchimp_url != "" -%}
                <form method="post" action="{{ settings.newslatter_mailchimp_url | default: '#' }}" id="mc-embedded-subscribe-form{{section.id}}" class="newletter" name="mc-embedded-subscribe-form" target="_blank">
                  <input type="email"
                         name="contact[email]"
                         id="contact_email{{section.id}}"
                         class="email-text required-entry validate-email"
                         value="{% if customer %}{{ customer.email }}{% endif %}"
                         placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}">
                  <span><button class="button" name="subscribe" id="subscribe{{section.id}}" type="submit">{{'general.newsletter_form.submit'|t}}</button></span>
                </form>
                {%- else -%}
                  {%-form 'customer', class: 'newletter'-%}
                  {{ form.errors | default_errors }}
                  {%-if form.posted_successfully?-%}
                  <p class="form--success">{{ 'general.newsletter_form.confirmation' | t }}</p>
                  {%-else-%}
                  <input type="hidden" name="contact[tags]" value="newsletter">
                  <input type="email"
                         name="contact[email]"
                         id="Email{{section.id}}"
                         class="email-text required-entry validate-email"
                         value="{%-if customer-%}{{ customer.email }}{%-endif-%}"
                         placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}">
                  <span><button class="button" type="submit" title="{{ 'general.newsletter_form.submit' | t }}">{{'general.newsletter_form.submit'|t}}</button></span>
                  {%-endif-%}
                  {%-endform-%}
                {%- endif -%}
              </div>
              <div class="widget widget_social {{block.settings.socials_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title2}}</h3>
                <div class="content">
                  {%- include "boutique_socials", social_grid: 'social' -%}
                </div>
              </div>
            </div>
          {%- elsif block.type == 'item_10' -%}
            {%-if block.settings.content_align == 'left' -%}
            {%- assign justify_content = 'start' -%}
            {%-elsif block.settings.content_align == 'right' -%}
            {%- assign justify_content = 'end' -%}
            {%-else-%}
            {%- assign justify_content = 'center' -%}
            {%-endif-%}
            <div class="footer-sidebar style_5 {{block.settings.block_style}} {{block_width | strip_newlines | remove: '   '}}" {{ block.shopify_attributes }}>
              <div class="widget widget_newletter {{block.settings.newsletter_design}} text-{{block.settings.content_align}}">
                <h3 class="widget-title">{{block.settings.title}}</h3>


                        <div class="klaviyo-form-PryZBa"></div>

<!--                 {%- if block.settings.text_newsletter != blank -%}<p>{{block.settings.text_newsletter}}</p>{%- endif -%}
