  list-style: none;
  position: relative;
}

.topbar-menu > li {
  display: inline-block;
}

.topbar-menu .sub-menu {
  position: absolute;
  left: auto;
  top: 100%;
  visibility: hidden;
  opacity: 0;
  filter: alpha(opacity=0);
  z-index: 10;
}

