  content: "\f107";
  font-family: 'FontAwesome';
  margin-left: 10px;
}

.topbar-menu li > a .fa {
  display: inline-block;
  margin-right: 10px;
}

.topbar-menu li > a img {
  display: inline-block;
  margin-right: 10px;
  margin-top: -2px;
}

.topbar-menu .sub-menu > li a {
  display: inline-block;
  padding: 5px 15px;
  display: inline-block;
  width: 100%;
  text-align: left;
  cursor: pointer;
}

.topbar-menu.right .sub-menu {
  right: 0;
  left: inherit;
}

.topbar-menu.right .sub-menu .sub-menu {
  right: 100%;
  top: 0;
}

