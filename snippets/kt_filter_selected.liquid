<ul class="filter-selected">
  {%-if current_tags-%}
  {% for current_tag in current_tags %}  
  {%- capture link -%}
  {{ current_tag | link_to_remove_tag: current_tag }}
  {%- endcapture -%}
  <li class="ul_layered_id_tags__selected-filter" style="margin-right: 0 !important;">
    <a data-pjax-filter href="{{link|split: 'href="'|last|split:'"'|first}}"><span class="remove_filterSelected" data-action="remove">{{current_tag|split: '  '|last}}<i class="pe-7s-close" aria-hidden="true"></i></span></a>
  </li>
  {% endfor %}
  {%-endif-%}
</ul>